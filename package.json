{
    "name": "gitbook-plugin-theme-gestalt-enhanced",
    "description": "A configurable theme for GitBook forked from gitbook-plugin-theme-gestalt https://github.com/antonlegoo/gitbook-plugin-theme-gestalt with a  new option",
    "main": "./index.js",
    "version": "1.0.1",
    "engines": {
        "gitbook": ">=3.0.0"
    },
    "devDependencies": {
        "browser-sync": "^2.18.13",
        "browserify": "13.1.0",
        "font-awesome": "^4.6.3",
        "gitbook": "^3.2.2",
        "gulp-browserify": "^0.5.1",
        "gulp-concat": "^2.6.1",
        "gulp-exec": "^2.1.3",
        "gulp-ignore": "^2.0.2",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^3.1.0",
        "gulp": "github:gulpjs/gulp#4.0",
        "jquery": "^3.1.1",
        "merge-stream": "^1.0.1",
        "mousetrap": "^1.6.1"
    },
    "scripts": {
        "start": "gulp build",
        "prepublish": "gulp build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bsvobodny/gitbook-plugin-theme-gestalt-enhanced"
    },
    "author": "Benjamin Svobodny <benjamin.svobodny@gmail.com>",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/bsvobodny/gitbook-plugin-theme-gestalt-enhanced/issues"
    },
    "gitbook": {
        "properties": {
            "styles": {
                "type": "object",
                "title": "Custom Stylesheets",
                "properties": {
                    "website": {
                        "title": "Stylesheet for website output",
                        "default": "styles/website.css"
                    }
                }
            },
            "showLevel": {
                "type": "boolean",
                "title": "Show level indicator in TOC",
                "default": false
            }
        }
    }
}
